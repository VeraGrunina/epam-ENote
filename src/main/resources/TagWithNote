CREATE TABLE `ENote`.`TagWithNote` (
  `note_id` INT NOT NULL,
  `tag_id` INT NOT NULL,
  INDEX `tag_id_idx` (`tag_id` ASC),
  INDEX `note_id_idx` (`note_id` ASC),
  CONSTRAINT `tag_id`
    FOREIGN KEY (`tag_id`)
    REFERENCES `ENote`.`Tag` (`tag_id`)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
  CONSTRAINT `note_id`
    FOREIGN KEY (`note_id`)
    REFERENCES `ENote`.`Note` (`note_id`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);