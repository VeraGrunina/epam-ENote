CREATE TABLE `ENote`.`Note` (
  `note_id` INT NOT NULL AUTO_INCREMENT,
  `notebook_id` INT NULL,
  `note_name` VARCHAR(45) NULL,
  PRIMARY KEY (`note_id`),
  INDEX `notebook_id_idx` (`notebook_id` ASC),
  CONSTRAINT `notebook_id`
    FOREIGN KEY (`notebook_id`)
    REFERENCES `ENote`.`Notebook` (`notebook_id`)
    ON DELETE CASCADE
    ON UPDATE CASCADE);